        <template><![CDATA[<div>
  <div style="display: grid; grid-template-columns: {{(tabset.align == 'top') ? 'auto' : (tabset.align == 'left') ? 'min-content auto' : 'auto min-content'}}; 
              grid-template-rows: {{(tabset.align == 'top') ? 'auto auto' : 'auto'}};" ng-init="navType = 'nav-' + tabset.type; navAlign = 'nav-' + tabset.align">
    <div style="grid-column: {{(tabset.align == 'right') ? '2' : '1'}}; grid-row: 1; overflow-y: auto;">
      <span class="tabs-label">{{tabset.label}}</span>
      <ul class="nav {{navType}} {{navAlign}} {{(vertical == true) ? 'nav-stacked' : ''}} {{(justified == true) ? 'nav-justified' : ''}}" ng-transclude></ul>
    </div>
    <div style="grid-column: {{(tabset.align == 'left') ? '2' : '1'}}; grid-row: {{(tabset.align == 'top') ? '2' : '1'}};" class="tab-content">
      <div class="tab-pane" ng-repeat="tab in tabset.tabs" ng-class="{active: tabset.active === tab.index}" uib-tab-content-transclude-v2="tab"></div>
    </div>
  </div>
</div>]]></template>
